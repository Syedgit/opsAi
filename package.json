{
  "name": "opsai",
  "version": "1.0.0",
  "description": "WhatsApp-based operations management system for convenience stores",
  "engines": {
    "node": ">=20.19.0"
  },
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon --exec ts-node src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "vercel-build": "prisma generate && npm run type-check && npm run build",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "seed": "ts-node scripts/seed-mock-data.ts",
    "lint": "eslint src api --ext .ts --max-warnings 0",
    "lint:fix": "eslint src api --ext .ts --fix",
    "type-check": "tsc --noEmit && tsc --noEmit --project tsconfig.api.json",
    "type-check:src": "tsc --noEmit",
    "type-check:api": "tsc --noEmit --project tsconfig.api.json",
    "format": "prettier --write \"src/**/*.ts\" \"api/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"api/**/*.ts\"",
    "check": "npm run type-check && npm run lint && npm run format:check",
    "check:api": "npm run type-check:api",
    "prebuild": "npm run type-check",
    "test": "jest"
  },
  "keywords": [
    "whatsapp",
    "operations",
    "convenience-store",
    "automation"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.450.0",
    "@google-cloud/vision": "^3.1.0",
    "@prisma/client": "^5.7.0",
    "axios": "^1.6.0",
    "bcryptjs": "^2.4.3",
    "bullmq": "^5.0.0",
    "date-fns": "^3.0.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "google-auth-library": "^9.15.1",
    "googleapis": "^128.0.0",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.0",
    "jsonwebtoken": "^9.0.3",
    "openai": "^4.20.0",
    "twilio": "^4.20.0",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.0",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "@vercel/node": "^5.5.27",
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.1",
    "prisma": "^5.7.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.0"
  }
}
