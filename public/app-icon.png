<svg width="1024" height="1024" xmlns="http://www.w3.org/2000/svg">
  <!-- Background -->
  <rect width="1024" height="1024" fill="#25D366" rx="200"/>
  
  <!-- WhatsApp-style chat bubble -->
  <path d="M 200 300 Q 200 250 250 250 L 550 250 Q 600 250 600 300 L 600 500 Q 600 550 550 550 L 350 550 L 250 650 L 250 550 Q 200 550 200 500 Z" fill="white" opacity="0.9"/>
  
  <!-- Store/shop icon inside -->
  <g transform="translate(350, 350)">
    <!-- Store building -->
    <rect x="50" y="100" width="200" height="150" fill="#25D366" rx="10"/>
    <rect x="70" y="120" width="40" height="60" fill="white" opacity="0.8"/>
    <rect x="130" y="120" width="40" height="60" fill="white" opacity="0.8"/>
    <rect x="190" y="120" width="40" height="60" fill="white" opacity="0.8"/>
    <!-- Roof -->
    <polygon points="0,100 150,0 300,100" fill="#1DA851"/>
    <!-- Door -->
    <rect x="120" y="180" width="60" height="70" fill="white" rx="5"/>
  </g>
  
  <!-- Text: opsAi -->
  <text x="512" y="800" font-family="Arial, sans-serif" font-size="120" font-weight="bold" fill="white" text-anchor="middle">opsAi</text>
  <text x="512" y="900" font-family="Arial, sans-serif" font-size="60" fill="white" text-anchor="middle" opacity="0.9">Store Management</text>
</svg>

